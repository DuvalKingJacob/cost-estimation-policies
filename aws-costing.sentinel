import "tfrun"
import "decimal"

monthly_cost = func() {
    if tfrun.cost_estimate.proposed_monthly_cost is null {
        return 0.0
    }
    return tfrun.cost_estimate.proposed_monthly_cost
}()

print("Proposed Monthly Cost: " + string(monthly_cost))

main = rule {
    decimal.new(monthly_cost).less_than(10)
}
